{
  "name": "whois-it",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:frontend": "pnpm --filter @whois-it/frontend dev",
    "dev:backend": "pnpm --filter @whois-it/backend start:dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r --parallel test:watch",
    "test:cov": "pnpm -r test:cov",
    "test:frontend": "pnpm --filter @whois-it/frontend test",
    "test:frontend:watch": "pnpm --filter @whois-it/frontend test:watch",
    "test:frontend:cov": "pnpm --filter @whois-it/frontend test:coverage",
    "test:backend": "pnpm --filter @whois-it/backend test",
    "test:backend:watch": "pnpm --filter @whois-it/backend test:watch",
    "test:backend:cov": "pnpm --filter @whois-it/backend test:cov",
    "test:backend:debug": "pnpm --filter @whois-it/backend test:debug",
    "test:backend:e2e": "pnpm --filter @whois-it/backend test:e2e",
    "seed": "pnpm --filter @whois-it/backend seed",
    "db:reset": "pnpm --filter @whois-it/backend db:reset",
    "migration:generate": "pnpm --filter @whois-it/backend migration:generate",
    "migration:run": "pnpm --filter @whois-it/backend migration:run",
    "migration:revert": "pnpm --filter @whois-it/backend migration:revert",
    "migration:show": "pnpm --filter @whois-it/backend migration:show"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.2",
    "@heroui/checkbox": "^2.3.27",
    "@iconify/react": "^6.0.2",
    "@types/negotiator": "^0.6.4",
    "negotiator": "^1.0.0",
    "server-only": "^0.0.1"
  }
}