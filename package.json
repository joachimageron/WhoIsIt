{
  "name": "whois-it",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.18.1",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:frontend": "pnpm --filter @whois-it/frontend dev",
    "dev:backend": "pnpm --filter @whois-it/backend start:dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "seed": "pnpm --filter @whois-it/backend seed",
    "db:reset": "pnpm --filter @whois-it/backend db:reset"
  },
  "dependencies": {
    "@heroui/checkbox": "^2.3.27",
    "@iconify/react": "^6.0.2"
  }
}
